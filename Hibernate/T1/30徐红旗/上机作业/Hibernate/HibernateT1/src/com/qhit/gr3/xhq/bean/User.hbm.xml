<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.qhit.gr3.cyh.Ht1.bean.User" table="t_user">
<id name="uid" column="uid" type="integer">
<generator class="native"></generator>
</id>

<property name="uname" column="uname" type="string"></property>
<property name="upwd" column="upwd" type="string"></property>
<property name="uage" column="uage" type="integer"></property>
<property name="birthday" column="birthday" type="string"></property>
</class>
</hibernate-mapping>
